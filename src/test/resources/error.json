{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "message",
        "code"
    ],
    "properties": {
        "message": {
            "type": "string",
            "default": "",
            "title": "The message Schema",
            "examples": [
                "Not Found"
            ]
        },
        "code": {
            "type": "integer",
            "default": 0,
            "title": "The code Schema",
            "examples": [
                404
            ]
        }
    },
    "examples": [{
        "message": "Not Found",
        "code": 404
    }]
}